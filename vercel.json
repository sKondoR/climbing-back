{
    "version": 2,
    "functions": {
        "src/*.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}